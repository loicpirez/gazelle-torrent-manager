{
  "format_path": "$artist/$date/$title/$format/$type/$quality",
  "download_path": "/torrents",
  "profile": {
    "jpopsuki.eu": {
      "regex": "(?<artist>((.*?))) - (?<title>((.*?))) - (?<date>([12]\\d{3}-(0[1-9]|1[0-2])-(0[1-9]|[12]\\d|3[01]))) \\((((?<format>.*?) - ((?<type>.*?)) - ((?<quality>.*?))))\\).torrent",
      "should_upload": true,
      "announce_url": "http://jpopsuki.eu:7531",
      "command_line": "transmission-cli torrent-add $fileName --download-dir $finalTarget"
    }
  }
}
